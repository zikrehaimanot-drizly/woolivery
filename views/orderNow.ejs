<!DOCTYPE HTML>
<html>
<head>
	<title>WooLivery</title>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
	<link rel="stylesheet" href="css/main.css" />
	<noscript><link rel="stylesheet" href="css/noscript.css" /></noscript>
</head>
<body class="is-preload">
<section class="containerPage">


	<!-- Wrapper -->
	<div id="wrapper">

		<!-- Header -->
		<header id="header">
			<a href="/generic" class="logo"><strong>Woo</strong> <span>Livery</span></a>
			<nav>
				<ul class="links">
					<!-- <li><a href="generic">Home</a></li> -->
				</ul>
			</nav>
		</header>

		<div id="main" class="alt">
			<p class="userId hide" > <%= user._id%> </p>

			<!-- Past Orders -->

			<section id="contact">

				<!-- Resturants To Order From -->
				<section class="split">
					<% for(var i=0; i<results.length; i++){%>
						<section>
							<h2>Resturant Name</h2>
							<span><%=results[i].resturantName %></span>
						</section>
						<section>
							<h2>Menu Items</h2>
							<ul>
								<% for(var j=0; j<results[i].menu.length; j++){%>
									<li class="resItem">
										<span><%= results[i].menu[j].itemName%></span>
										<span> <%=results[i].menu[j].price%></span>
										<span><%=results[i].createdBy %></span>
										<span><%=results[i]._id %></span>

									</li>
									<% } %>
								</ul>
							</section>
							<% } %>

						</section>
						<section>

							<ul class="cart">
							</ul>
							<p class="displayPrice"></p>
							<button type="submit" name="button" class="hide checkout">CheckOut</button>
							<button type="submit" name="button" class="clearCart"> Clear Cart</button>
						</section>
					</div>
				</section>
</section>
				<!-- Footer -->
				<footer id="footer">
					<div class="inner">
						<ul class="icons">
							<li><a href="#" class="icon brands alt fa-twitter"><span class="label">Twitter</span></a></li>
							<li><a href="#" class="icon brands alt fa-facebook-f"><span class="label">Facebook</span></a></li>
							<li><a href="#" class="icon brands alt fa-instagram"><span class="label">Instagram</span></a></li>
							<li><a href="#" class="icon brands alt fa-github"><span class="label">GitHub</span></a></li>
							<li><a href="#" class="icon brands alt fa-linkedin-in"><span class="label">LinkedIn</span></a></li>
						</ul>
						<ul class="copyright">
						</ul>
					</div>
				</footer>
			</div>

			<script src="orderNow.js" ></script>

		</body>
		</html>
